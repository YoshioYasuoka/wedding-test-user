<section>
    <div class="container">
        <p><%= link_to 'New', :new_example, class: 'btn btn-sm btn-success' %>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Name</th>
              <th>design</th>
              <th>album</th>
              <th>pageview</th>
              <th>like</th>
              <th>action</th>
            </tr>
          </thead>
          <tbody>
            <% @examples.each do |example| %>
            <tr>
                <td><%= link_to example.name, example %></td>
                <td><%= example.design %></td>
                <td><%= example.album %></td>
                <td><%= example.pageview %></td>
                <td><%= example.like %></td>
                <td>
                    <%= link_to "Edit", [:edit, example], class: 'btn btn-sm btn-primary' %>
                    <%= link_to "Delete", example, method: :delete, class: 'btn btn-sm btn-danger', data:{confirm: '本当に削除しますか？'} %>
                </td>
            </tr>
            <% end %>
          
          
        </table>
        
        
    </div>
</section>
